    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "CodeLens (Launch) - GUI",
            "request": "launch",
            "vmArgs": "--module-path D:/JavaIn/javafx-sdk-11.0.2/lib --add-modules javafx.controls,javafx.fxml",
            "mainClass": "com.mycompany.app.GUI.GUI",
            "projectName": "my-app"
        },
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "vmArgs": "--module-path D:/JavaIn/javafx-sdk-11.0.2/lib --add-modules javafx.controls,javafx.fxml",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "vmArgs": "--module-path D:/JavaIn/javafx-sdk-11.0.2/lib --add-modules javafx.controls,javafx.fxml",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
           "vmArgs": "--module-path D:/JavaIn/javafx-sdk-11.0.2/lib --add-modules javafx.controls,javafx.fxml",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-App<my-app>",
            "request": "launch",
            "vmArgs": "--module-path D:/JavaIn/javafx-sdk-11.0.2/lib --add-modules javafx.controls,javafx.fxml",
            "mainClass": "com.mycompany.app.App",
            "projectName": "my-app"
        }
    ]
}